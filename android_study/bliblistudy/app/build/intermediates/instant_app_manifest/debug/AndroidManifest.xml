<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.blibli.myapplication"
    android:targetSandboxVersion="2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="29" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.REQUEST_INSTALL_PACKAGES" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" /> <!-- 写文件的权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.blibli.myapplication.ExpandableListView.ExpandableListViewActivity" />
        <activity android:name="com.blibli.myapplication.SQL.SQLActivity" />
        <activity android:name="com.blibli.myapplication.AsyncTask.AsyncTaskActivity" />
        <activity android:name="com.blibli.myapplication.handle.DownLoadActivity" />
        <activity android:name="com.blibli.myapplication.handle.HandlerActivity" />
        <activity android:name="com.blibli.myapplication.progressActivity" />
        <activity android:name="com.blibli.myapplication.DialogActivity" />
        <activity android:name="com.blibli.myapplication.ToastActivity" />
        <activity android:name="com.blibli.myapplication.WebView.WebViewActivity" />
        <activity android:name="com.blibli.myapplication.recycleview.LinearRecyclerViewActivity" />
        <activity android:name="com.blibli.myapplication.recycleview.RecyclerViewActivity" />
        <activity android:name="com.blibli.myapplication.listView.ListViewActivity" />
        <activity android:name="com.blibli.myapplication.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.blibli.myapplication.girdview.GirdViewActivity" />
    </application>

</manifest>